{
  "name": "VOEC Listed",
  "description": "Check if your current website is listed under VOEC!",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage", "activeTab", "tabs", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/voec_icon_16.png",
      "32": "/images/voec_icon_32.png",
      "48": "/images/voec_icon_48.png",
      "128": "/images/voec_icon_128.png"
    }
  },
  "icons": {
    "16": "/images/voec_icon_16.png",
    "32": "/images/voec_icon_32.png",
    "48": "/images/voec_icon_48.png",
    "128": "/images/voec_icon_128.png"
  }
}